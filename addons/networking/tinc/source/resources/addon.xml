<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="service.system.%NETWORK%"
       name="%HOST%"
       version="%VERSION%"
       provider-name="%NETWORK%">
  <requires>
    <import addon="os.openelec.tv" version="4.0"/>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="service.system.tinc" version="4.1.3"/>
  </requires>
  <extension point="xbmc.service"
             library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Host %HOST% of the %NETWORK% tinc network.</summary>
    <description>Host %HOST% of the %NETWORK% tinc network.</description>
    <disclaimer></disclaimer>
    <platform>all</platform>
  </extension>
</addon>
